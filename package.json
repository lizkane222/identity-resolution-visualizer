{
  "name": "identity_resolution_visualizer",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.6.2",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "html2canvas": "^1.4.1",
    "papaparse": "^5.5.3",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-scripts": "^5.0.1",
    "twilio": "^5.10.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "server": "node server/server.js",
    "dev": "concurrently \"npm run server\" \"npm start\"",
    "dev-prod": "concurrently \"npm run server\" \"npm run serve-build\"",
    "serve-build": "serve -s build -l 3000",
    "stop": "pkill -f \"node server/server.js\" && pkill -f \"react-scripts start\"",
    "stop-server": "pkill -f \"node server/server.js\"",
    "stop-client": "pkill -f \"react-scripts start\"",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "setup": "node scripts/setup-env.js",
    "postinstall": "node scripts/setup-env.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:8888"
}
